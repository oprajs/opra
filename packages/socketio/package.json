{
  "name": "@opra/socketio",
  "version": "1.22.4",
  "description": "Opra Socket.IO adapter",
  "author": "Panates",
  "license": "MIT",
  "scripts": {
    "compile": "tsc --noEmit",
    "prebuild": "npm run check && npm run lint && npm run clean",
    "build": "tsc -b tsconfig-build.json",
    "postbuild": "node ../../support/postbuild.cjs",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --max-warnings=0 --fix",
    "format": "prettier . --write --log-level=warn",
    "check": "madge --circular src/**",
    "test": "mocha",
    "clean": "npm run clean:src && npm run clean:test && npm run clean:build && npm run clean:cover",
    "clean:src": "ts-cleanup -s src --all",
    "clean:test": "ts-cleanup -s test --all",
    "clean:build": "rimraf build *.tsbuildinfo",
    "clean:cover": "rimraf ../../coverage/socketio"
  },
  "dependencies": {
    "@jsopen/objects": "^2.0.2",
    "@browsery/type-is": "^2.0.1",
    "content-type": "^1.0.5",
    "iconv-lite": "^0.7.1",
    "node-events-async": "^1.5.0",
    "tslib": "^2.8.1",
    "valgen": "^5.18.2"
  },
  "peerDependencies": {
    "@opra/common": "^1.22.4",
    "@opra/core": "^1.22.4",
    "socket.io": ">=4.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.2.0",
    "@types/accepts": "^1.3.7",
    "expect": "^30.2.0",
    "ts-gems": "^3.11.3"
  },
  "type": "module",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=20.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/panates/opra.git",
    "directory": "packages/socketio"
  },
  "keywords": [
    "opra",
    "socketio",
    "websocket",
    "ws"
  ],
  "publishConfig": {
    "access": "public"
  }
}