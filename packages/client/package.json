{
  "name": "@opra/client",
  "version": "0.28.1",
  "description": "Opra Client package",
  "author": "Panates",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/panates/opra.git",
    "directory": "packages/client"
  },
  "scripts": {
    "compile": "tsc",
    "prebuild": "npm run check && npm run lint && npm run clean",
    "build": "npm run build:cjs && npm run build:esm && npm run build:browser",
    "build:cjs": "tsc -b tsconfig-build-cjs.json",
    "build:esm": "tsc -b tsconfig-build-esm.json",
    "build:browser": "node esbuild.mjs",
    "postbuild": "cp README.md package.json ../../LICENSE ../../build/client && cp ../../package.cjs.json ../../build/client/cjs/package.json",
    "lint": "eslint . --max-warnings=0",
    "check": "madge --circular src/**",
    "test": "jest",
    "cover": "jest --collect-coverage",
    "clean": "npm run clean:src && npm run clean:dist && npm run clean:cover",
    "clean:src": "ts-cleanup -s src --all",
    "clean:dist": "rimraf ../../build/client",
    "clean:cover": "rimraf ../../coverage/client"
  },
  "type": "module",
  "module": "./esm/index.js",
  "main": "./cjs/index.js",
  "browser": "./browser.js",
  "types": "./types/index.d.ts",
  "dependencies": {
    "@browsery/fs": "^0.4.0",
    "@browsery/highland": "^0.6.0",
    "@browsery/http-parser": "^0.4.0",
    "@browsery/i18next": "^0.7.2",
    "@browsery/stream": "^0.6.0",
    "@browsery/type-is": "^0.6.3",
    "@browsery/util": "^0.4.0",
    "@opra/common": "^0.28.1",
    "accepts": "^1.3.8",
    "buffer": "^6.0.3",
    "cookie": "^0.5.0",
    "crypto-browserify": "^3.12.0",
    "dayjs": "^1.11.10",
    "encodeurl": "^1.0.2",
    "events": "^3.3.0",
    "fast-tokenizer": "^1.2.2",
    "lodash.omit": "^4.5.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "putil-isplainobject": "^1.1.5",
    "putil-merge": "^3.12.1",
    "putil-promisify": "^1.10.1",
    "putil-varhelpers": "^1.6.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": ">=7.0.0",
    "ts-lib": "^0.0.5",
    "uid": "^2.0.1"
  },
  "sideEffects": false,
  "files": [
    "cjs/",
    "esm/",
    "types/",
    "browser.js",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "opra",
    "node",
    "client",
    "browser",
    "http",
    "request",
    "fetch"
  ]
}