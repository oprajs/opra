{
  "name": "@opra/client",
  "version": "0.15.0",
  "description": "Opra Client package",
  "author": "Panates",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/panates/opra.git",
    "directory": "packages/client"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "rollup -c rollup.config.mjs",
    "clean": "rimraf ../../build/client"
  },
  "type": "module",
  "module": "./esm/index.min.mjs",
  "main": "./cjs/index.min.cjs",
  "types": "./types/index.d.ts",
  "typings": "./types/index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "require": "./cjs/index.min.cjs",
      "default": "./esm/index.min.mjs"
    }
  },
  "dependencies": {
    "@browsery/antlr4": "^0.2.0",
    "@browsery/fs": "^0.2.0",
    "@browsery/highland": "^0.2.0",
    "@browsery/http-parser": "^0.2.0",
    "@browsery/i18next": "^0.2.0",
    "@browsery/stream": "^0.2.0",
    "@browsery/util": "^0.2.0",
    "accepts": "^1.3.8",
    "buffer": "^6.0.3",
    "cookie": "^0.5.0",
    "crypto-browserify": "^3.12.0",
    "dayjs": "^1.11.7",
    "encodeurl": "^1.0.2",
    "events": "^3.3.0",
    "fast-tokenizer": "^1.2.2",
    "lodash.omit": "^4.5.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "putil-isplainobject": "^1.1.5",
    "putil-merge": "^3.10.3",
    "putil-promisify": "^1.10.0",
    "putil-varhelpers": "^1.6.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "ts-lib": "^0.0.5",
    "type-is": "^1.6.18",
    "uid": "^2.0.1"
  },
  "devDependencies": {
    "@opra/node-client": "^0.15.0"
  },
  "sideEffects": false,
  "files": [
    "cjs/",
    "esm/",
    "types/",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "opra",
    "node",
    "client",
    "browser",
    "http",
    "request",
    "fetch"
  ]
}
