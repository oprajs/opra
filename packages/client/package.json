{
  "name": "@opra/client",
  "version": "0.10.0",
  "description": "Opra Client package",
  "author": "Panates",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/panates/opra.git",
    "directory": "packages/client"
  },
  "scripts": {
    "compile": "tsc",
    "prebuild": "npm run clean",
    "build": "node scripts/bundle.cjs",
    "clean": "npm run clean:src && npm run clean:dist && npm run clean:cover",
    "clean:src": "ts-cleanup -s src --all",
    "clean:dist": "rimraf ../../build/node-client ../../build/client",
    "clean:cover": "rimraf ../../coverage/client",
    "bundle": "node support/bundle.cjs"
  },
  "type": "module",
  "module": "./esm/main.min.mjs",
  "main": "./cjs/main.min.cjs",
  "types": "./typings/index.d.ts",
  "typings": "./typings/index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "require": "./cjs/main.min.cjs",
      "default": "./esm/main.min.mjs"
    }
  },
  "dependencies": {
    "@opra/node-client": "^0.10.0",
    "fast-tokenizer": "^1.2.0",
    "i18next": "^22.4.6",
    "lodash-es": "^4.17.21",
    "luxon": "^3.1.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.6.0",
    "uid": "^2.0.1"
  },
  "devDependencies": {
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "path-browserify": "^1.0.1",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5",
    "url": "^0.11.0",
    "ts-gems": "^2.3.0"
  },
  "sideEffects": false,
  "files": [
    "typings/",
    "cjs/",
    "esm/",
    "umd/",
    "typings/",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "opra",
    "node",
    "client",
    "browser",
    "http",
    "request",
    "fetch"
  ]
}