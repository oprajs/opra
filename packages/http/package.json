{
  "name": "@opra/http",
  "version": "1.22.4",
  "description": "Opra Http Server Adapter",
  "author": "Panates",
  "license": "MIT",
  "scripts": {
    "compile": "tsc --noEmit",
    "prebuild": "npm run check && npm run lint && npm run clean",
    "build": "tsc -b tsconfig-build.json",
    "postbuild": "node ../../support/postbuild.cjs && cp -R i18n ./build/i18n",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --max-warnings=0 --fix",
    "format": "prettier . --write --log-level=warn",
    "check": "madge --circular src/**",
    "test": "mocha",
    "clean": "npm run clean:src && npm run clean:test && npm run clean:build && npm run clean:cover",
    "clean:src": "ts-cleanup -s src --all",
    "clean:test": "ts-cleanup -s test --all",
    "clean:build": "rimraf build *.tsbuildinfo",
    "clean:cover": "rimraf ../../coverage/http"
  },
  "dependencies": {
    "@browsery/antlr4": "^4.13.3-r4",
    "@browsery/http-parser": "^0.5.10-r2",
    "@browsery/type-is": "^2.0.1",
    "@jsopen/objects": "^2.0.2",
    "accepts": "^1.3.8",
    "base64-stream": "^1.0.0",
    "busboy": "^1.6.0",
    "bytes": "^3.1.2",
    "content-disposition": "^1.0.1",
    "content-type": "^1.0.5",
    "cookie": "^1.1.1",
    "cookie-signature": "^1.2.2",
    "encodeurl": "^2.0.0",
    "expect": "^30.2.0",
    "fast-tokenizer": "^1.8.0",
    "fresh": "^0.5.2",
    "iconv-lite": "^0.7.1",
    "mime-types": "^3.0.2",
    "node-events-async": "^1.5.0",
    "power-tasks": "^1.11.1",
    "putil-varhelpers": "^1.6.5",
    "range-parser": "^1.2.1",
    "raw-body": "^3.0.2",
    "reflect-metadata": "^0.2.2",
    "super-fast-md5": "^1.0.3",
    "tslib": "^2.8.1",
    "valgen": "^5.18.2",
    "vary": "^1.1.2"
  },
  "peerDependencies": {
    "@opra/common": "^1.22.4",
    "@opra/core": "^1.22.4"
  },
  "optionalDependencies": {
    "express": "^4.0.0 || ^5.0.0",
    "fastify": "^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.2.0",
    "@types/accepts": "^1.3.7",
    "@types/base64-stream": "^1.0.5",
    "@types/busboy": "^1.5.4",
    "@types/bytes": "^3.1.5",
    "@types/content-disposition": "^0.5.9",
    "@types/content-type": "^1.1.9",
    "@types/cookie-parser": "^1.4.10",
    "@types/cookie-signature": "^1.1.2",
    "@types/encodeurl": "^1.0.3",
    "@types/express": "^5.0.6",
    "@types/fresh": "^0.5.3",
    "@types/mime-types": "^3.0.1",
    "@types/range-parser": "^1.2.7",
    "@types/vary": "^1.1.3",
    "cookie-parser": "^1.4.7",
    "crypto-browserify": "^3.12.1",
    "express": "^5.1.0",
    "fastify": "^5.2.1",
    "path-browserify": "^1.0.1",
    "supertest": "^7.1.4",
    "ts-gems": "^3.11.3"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./package.json": "./package.json"
  },
  "type": "module",
  "module": "./index.js",
  "types": "./index.d.ts",
  "engines": {
    "node": ">=20.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/panates/opra.git",
    "directory": "packages/http"
  },
  "keywords": [
    "opra",
    "rest",
    "api",
    "openapi",
    "http",
    "web",
    "swagger",
    "raml"
  ],
  "publishConfig": {
    "access": "public"
  }
}